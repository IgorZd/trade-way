{
  "id": "blog-post",
  "label": "Blog Post",
  "repeatable": true,
  "status": true,
  "json": {
    "Main": {
      "publishDate": {
        "type": "Date",
        "config": {
          "label": "Publish Date",
          "placeholder": ""
        }
      },
      "title": {
        "type": "StructuredText",
        "config": {
          "label": "Title",
          "placeholder": "",
          "allowTargetBlank": true,
          "single": "heading1,heading2,heading3,heading4,heading5,heading6"
        }
      },
      "content": {
        "type": "StructuredText",
        "config": {
          "label": "Content",
          "placeholder": "",
          "allowTargetBlank": true,
          "multi": "paragraph,preformatted,strong,em,hyperlink,list-item,o-list-item"
        }
      },
      "uid": {
        "type": "UID",
        "config": {
          "label": "slug",
          "placeholder": ""
        }
      },
      "image": {
        "type": "Image",
        "config": {
          "label": "Image",
          "constraint": {},
          "thumbnails": []
        }
      }
    }
  }
}